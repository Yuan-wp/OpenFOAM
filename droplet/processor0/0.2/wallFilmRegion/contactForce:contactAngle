FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.2/wallFilmRegion";
    object      contactForce:contactAngle;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    wallFilmFace_top
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    region0_to_wallFilmRegion_wallFilmFace
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    mirrorFace_top
    {
        type            calculated;
        value           nonuniform List<vector> 0();
    }
    region0_to_wallFilmRegion_mirrorFace
    {
        type            calculated;
        value           nonuniform List<vector> 0();
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


