FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//限制温度在200k-5000k之间 
limitT
{
    type            limitTemperature;
    active          yes;
    selectionMode   all;
    min             200;
    max             5000;
}


