FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    walls
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    region0_to_wallFilmRegion_wallFilmFace
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    region0_to_wallFilmRegion_mirrorFace
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 100000;
    }
}


