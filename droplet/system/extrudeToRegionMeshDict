FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      extrudeToRegionMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//拉伸出液膜区域  拉伸边界默认为向计算域外拉伸
region          wallFilmRegion;  //区域名称

faceZones           //拉伸源表面集合
(
    wallFilmFace
    mirrorFace
);

oneD            false;

sampleMode      nearestPatchFace; //近壁面

extrudeModel    linearNormal; //线性

nLayers         1;   //拉伸一层

expansionRatio  1;  

adaptMesh       yes; // apply mapped to both regions

linearNormalCoeffs
{
    thickness       0.001;  //拉伸厚度
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
